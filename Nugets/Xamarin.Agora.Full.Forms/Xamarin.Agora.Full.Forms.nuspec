    <!-- Xamarin.iOS -->
    <file src="lib/Xamarin.iOS10/Xamarin.Agora.Full.Forms*.dll" target="lib/Xamarin.iOS10" />
    <!-- Xamarin.Android -->
    <file src="lib/MonoAndroid10/Xamarin.Agora.Full.Forms*.dll" target="lib/MonoAndroid10" />
    <!--Xamarin.Mac-->
    <file src="lib/Xamarin.Mac20/Xamarin.Agora.Full.Forms*.dll" target="lib/Xamarin.Mac20" />
  </files>
</package>
