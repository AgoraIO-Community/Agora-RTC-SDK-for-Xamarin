<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Xamarin.Agora.Full.Forms</id>
    <title>Agora Xamarin SDK for Xamarin.Forms. Supports iOS, Android and MacOS</title>
    <version>2.3.3</version>
    <authors>dreamteammobile</authors>
    <owners>dreamteammobile</owners>
    <projectUrl>https://github.com/AgoraIO-Community/Agora-RTC-SDK-for-Xamarin</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Agora Xamarin SDK for Xamarin.Forms. Supports iOS, Android and MacOS</description>
